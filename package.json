{
  "name": "kilo-rn-boilerplate",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "allow-bash": "chmod -R 777 ./scripts",
    "android": "react-native run-android",
    "android-emulator": "./scripts/runAndroidEmulator.sh",
    "clean": "rm -rf node_modules && yarn install && cd ios && rm -rf Pods && pod install && cd ../android && ./gradlew clean",
    "clean-gradle": "cd android && ./gradlew clean cleanBuildCache",
    "clean-pods": "cd ios && pod deintegrate && pod install",
    "fastlane-install": "fastlane install_plugins",
    "inc": "./scripts/incVersion.sh",
    "postinstall": "yarn allow-bash && yarn jetify",
    "ios": "react-native run-ios",
    "ios11": "react-native run-ios --simulator='iPhone 11'",
    "ios6": "react-native run-ios --simulator='iPhone 6s'",
    "lint": "esw src --color",
    "lint-fix": "eslint --fix src",
    "match-development": "fastlane match development",
    "match-production": "fastlane match appstore",
    "pod-install": "cd ios && pod install",
    "precommit": "lint-staged && yarn sort-package-json && git add package.json",
    "prettier": "prettier --write '{src,__tests__}/**/*.{js,jsx}'",
    "release-android-prod": "fastlane android internal_prod",
    "release-android-stage": "fastlane android internal_stage",
    "release-ios-prod": "fastlane ios prod",
    "release-ios-stage": "fastlane ios stage",
    "rename": "yarn react-native-rename",
    "restart": "yarn && yarn pod-install && yarn start",
    "start": "react-native start",
    "test": "jest --config='./jest.config.json' --detectOpenHandles",
    "test-cov": "jest --config='./jest.config.json' --collectCoverageFrom='src/**/*.js'",
    "version-app": "yarn react-native-version -r -A -t ios,android"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn prettier",
      "yarn lint-fix",
      "yarn lint",
      "git add"
    ],
    "src/**/__tests__/**/*.{js,jsx}": [
      "yarn test-cov",
      "node ./node_modules/.bin/jest-badges-readme",
      "git add 'README.md'"
    ]
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-native-firebase/app": "11.2.0",
    "@react-native-firebase/auth": "11.2.0",
    "@react-native-firebase/remote-config": "11.2.0",
    "@react-navigation/bottom-tabs": "^5.11.10",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.4",
    "@sentry/react-native": "^2.4.0",
    "axios": "^0.21.1",
    "formik": "^2.2.6",
    "i18next": "^20.2.1",
    "immutability-helper": "^3.1.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-i18next": "^11.8.13",
    "react-native": "0.63.4",
    "react-native-date-picker": "^2.7.8",
    "react-native-device-info": "8.1.0",
    "react-native-fast-image": "^8.3.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-progress": "^1.1.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-reanimated": "^1.13.3",
    "react-native-render-html": "^5.1.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.1.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.1-0",
    "react-native-svg-charts": "^5.4.0",
    "react-native-vector-icons": "^8.1.0",
    "react-native-webview": "^11.4.0",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/runtime": "^7.13.10",
    "@jest/reporters": "^25.2.4",
    "@olavoparno/jest-badges-readme": "^1.3.6",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.4",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.10.0",
    "eslint-watch": "^7.0.0",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "lint-staged": "^10.5.4",
    "metro-react-native-babel-preset": "^0.59.0",
    "mitt": "^2.1.0",
    "prettier": "^2.2.1",
    "query-string": "^7.0.0",
    "react-dom": "^16.13.1",
    "react-native-rename": "^2.8.0",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "16.13.1",
    "reactotron-core-client": "^2.8.10",
    "reactotron-react-native": "^5.0.0",
    "reactotron-redux": "^3.1.3",
    "reactotron-redux-saga": "^4.2.3",
    "rn-host-detect": "^1.2.0",
    "sort-package-json": "^1.49.0"
  }
}
